@{
    ViewBag.Title = "Grid2";
}
<script src="~/Scripts/jquery.jqGrid.min.js" type="text/javascript"></script>
<link href="~/Content/jquery.jqGrid/ui.jqgrid.css" rel="stylesheet" type="text/css" />
<link href="http://trirand.com/blog/jqgrid/themes/redmond/jquery-ui-custom.css" rel="stylesheet" type="text/css" />
<script type="text/javascript">
    $(function() {
        $("#list2").jqGrid({
            url: '@Url.Action("List")',
            datatype: "json",
            //colNames: ['CityID', 'ZipCode', 'ProvinceID', 'DateCreated'],
            //colModel: [
            //    { name: 'CityID', index: 'CityID', width: 55 },
            //    { name: 'ZipCode', index: 'ZipCode', width: 90 },
            //    { name: 'ProvinceID', index: 'ProvinceID', width: 100 },
            //    { name: 'DateCreated', index: 'DateCreated', width: 80, align: "right" },
            //],
            colNames: ['CityID', 'ZipCode'],
            colModel: [
                { name: 'id', index: 'id', width: 55 },
                { name: 'ZipCode', index: 'ZipCode', width: 90 }
            ],
            rowNum: 10,
            rowList: [10, 20, 30],
            pager: '#pager2',
            sortname: 'id',
            viewrecords: true,
            sortorder: "desc",
            caption: "JSON Example",
            jsonReader: {
                repeatitems: false,
                id: "0"
            },
        });
        $("#list2").jqGrid('navGrid', '#pager2', { edit: false, add: false, del: false });

    });
    
</script>
 <table id="list2"></table>
<div id="pager2"></div>

